	PrepareSound(IntensedReloadSoundFilesPath[2], sizeof(IntensedReloadSoundFilesPath[]));
	PrepareSound(IntensedReloadSoundFilesPath[3], sizeof(IntensedReloadSoundFilesPath[]));
	
	
	/*** 센트리건 관련 ***/
	PrecacheModel("models/chicken/chicken.mdl");
	g_nBeamEntModel = PrecacheModel(LASER);
	
	PrecacheEffect("Impact");
