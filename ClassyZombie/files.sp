	AddFileToDownloadsTable(REDCROSS_VTF);
	
	g_iExplosion = PrecacheModel("sprites/sprite_fire01.vmt");
	PrecacheModel(MODEL_SUPPLY_CRATE, true);
	
	PrecacheModel("particle/particle_smokegrenade1.vmt");
	PrecacheModel("particle/particle_smokegrenade2.vmt");
	PrecacheModel("particle/particle_smokegrenade3.vmt");
	PrecacheModel(BarricadeModel);
	
	char downloadPath[256];
	FormatEx(downloadPath, sizeof(downloadPath), "sound/%s", SOUND_HOST_ZOMBIE_SKILL);
	PrecacheSoundAny(SOUND_HOST_ZOMBIE_SKILL);
	AddFileToDownloadsTable(downloadPath);
	
