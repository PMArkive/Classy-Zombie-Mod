	PrepareModel(CommonZombieModelFilesPath1, sizeof(CommonZombieModelFilesPath1));
	PrepareModel(CommonZombieModelFilesPath2, sizeof(CommonZombieModelFilesPath2));
	PrepareModel(CommonZombieModelFilesPath3, sizeof(CommonZombieModelFilesPath3));
	PrepareModel(CommonZombieModelFilesPath4, sizeof(CommonZombieModelFilesPath4));
	PrepareModel(CommonZombieModelFilesPath5, sizeof(CommonZombieModelFilesPath5));
	
	PrepareModel(ZombieAmrsModelFilesPath[0], sizeof(ZombieAmrsModelFilesPath[]));
	PrepareModel(ZombieAmrsModelFilesPath[1], sizeof(ZombieAmrsModelFilesPath[]));
	
	PrepareSound(CoughSoundFilesPath[0], sizeof(CoughSoundFilesPath[]));
