void PrepareModel(const char [][]path, int arraysize)
{
	for (int i = 0; i < arraysize;i++)
	{
		if (StrEqual(path[i], NULL_STRING))	continue;
		
		if(StrContains(path[i], ".mdl", false) != -1)
			PrecacheModel(path[i], true);
		
		AddFileToDownloadsTable(path[i]);
		
	}
}

void PrepareSound(const char [][]path, int arraysize)
{
	for (int i = 0; i < arraysize;i++)
	{
		if (StrEqual(path[i], NULL_STRING))	continue;
		
		PrecacheSoundAny(path[i], false);
		char downloadPath[256];
		FormatEx(downloadPath, sizeof(downloadPath), "sound/%s", path[i]);
		AddFileToDownloadsTable(downloadPath);
	}
}

