	AddFileToDownloadsTable(downloadPath);
	
	PrecacheSoundAny(SOUND_COMMON_ZOMBIE_SPAWN);
	FormatEx(downloadPath, sizeof(downloadPath), "sound/%s", SOUND_COMMON_ZOMBIE_SPAWN);
	AddFileToDownloadsTable(downloadPath);
	
	PrecacheSoundAny(ITEM_SOUND_NAROHO);
	FormatEx(downloadPath, sizeof(downloadPath), "sound/%s", ITEM_SOUND_NAROHO);
	AddFileToDownloadsTable(downloadPath);
	
	PrecacheSoundAny(AmmoSetSound);
	PrecacheSoundAny(HealingSound);
	PrecacheSoundAny(SOUND_BECOME_SPECIAL_ZOMBIE);
	
	PrepareModel(AmmoBoxModelFilesPath, sizeof(AmmoBoxModelFilesPath));	
	PrepareModel(HostZombieModelFilesPath, sizeof(HostZombieModelFilesPath));
